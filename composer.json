{
	"name" : "zephir/luya-module-admin",
	"description" : "Zephir Luya Admin",
	"require" : {
		"zephir/luya" : "*",
        "bower-asset/jquery" : "2.*",
        "bower-asset/jquery-ui" : "1.*",
        "bower-asset/angular" : "1.3.*",
        "bower-asset/angular-resource": "1.3.*",
        "bower-asset/angular-ui-router" : "0.2.*",
        "bower-asset/angular-loading-bar" : "0.*",
        "bower-asset/angular-ui-ace" : "*",
        "bower-asset/angular-dragdrop" : "1.*",
        "bower-asset/ace-builds" : "1.1.8"
	},
	"autoload" : {
		"psr-4" : {
			"admin\\" : "src/"
		}
	},
	"extra": {
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}
